<div class="form_container">
  <form method="POST" action="/add" enctype="multipart/form-data">
    <h3 class="form_title">ADD ASSOCIATION</h3>
    <div class="form_item_wrapper flexed_item">
      <label for="asso_name" class="label">Association's name</label>
      <input value="Les restos du coeur" type="text" class="input" placeholder="ex: Les restos du coeur">
    </div>
    <div class="form_item_wrapper flexed_item">
      <label for="asso_date" class="label">Creation's date</label>
      <input value="2009" type="text" class="input" placeholder="ex: 2009">
    </div class="flexed_item">
    <div class="form_item_wrapper flexed_item">
      <label for="asso_do" class="label">What they do</label>
      <input value="they make good things" type="text" class="input" placeholder="ex: They protect animals">
    </div>
    <div class="form_item_wrapper flexed_item">
      <label for="asso_last" class="label">Last mission</label>
      <input value="go on mars" type="text" class="input" placeholder="ex: They created a new school">
    </div>
    <div class="form_item_wrapper flexed_item">
      <label for="asso_financial" class="label">Financial report</label>
      <input value="link" type="text" class="input" placeholder="Link to their financial report">
    </div>
    <div class="form_item_wrapper flexed_item">
      <label for="asso_category" class="label">Category</label>
      <select name="category" id="main_category" class="select">
        <option value="">Choose the association's category</option>
        <option value="">Culture</option>
        <option value="">Enviro</option>
        <option value="">Health</option>
        <option value="">Solida</option>
      </select>
    </div>
    <div class="form_item_wrapper flexed_item">
      <label for="asso_image" class="label">Image</label>
      <input class="input" type="file">
    </div>
    <button>Add this association</button>
  </form>
</div>